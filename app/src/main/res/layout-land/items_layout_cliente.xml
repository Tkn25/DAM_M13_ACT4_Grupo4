<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:layout_margin="0.1dp"
    android:padding="16dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView10"
            android:layout_width="175dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/lemonmilk"
            android:text="Tipo:"
            android:textSize="13dp" />

        <TextView
            android:id="@+id/textViewTipoItem"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/lemonmilklight"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView11"
            android:layout_width="175dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/lemonmilk"
            android:textSize="13dp"
            android:text="USUARIO: " />

        <TextView
            android:id="@+id/textViewUsuarioItem"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/lemonmilklight"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView12"
            android:layout_width="175dp"
            android:layout_height="match_parent"
            android:fontFamily="@font/lemonmilk"
            android:textSize="13dp"
            android:text="DNI: " />

        <TextView
            android:id="@+id/textViewDNIItem"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/lemonmilklight"
            android:layout_weight="1" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView13"
            android:layout_width="175dp"
            android:layout_height="match_parent"
            android:fontFamily="@font/lemonmilk"
            android:textSize="13dp"
            android:text="NOMBRE: " />

        <TextView
            android:id="@+id/textViewNombreUsuItem"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/lemonmilklight"
            android:layout_weight="1" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView14"
            android:layout_width="175dp"
            android:layout_height="match_parent"
            android:fontFamily="@font/lemonmilk"
            android:textSize="13dp"
            android:text="TELEFONO: " />

        <TextView
            android:id="@+id/textViewTelItem"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/lemonmilklight"
            android:layout_weight="1" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView15"
            android:layout_width="175dp"
            android:layout_height="match_parent"
            android:fontFamily="@font/lemonmilk"
            android:textSize="13dp"
            android:text="DIRECCIÓN: " />

        <TextView
            android:id="@+id/textViewDirItem"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/lemonmilklight"
            android:layout_weight="1" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂"
        android:textSize="14sp" />

</LinearLayout>